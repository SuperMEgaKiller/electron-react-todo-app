{
  "name": "reactify-electron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts": "1.1.5"
  },
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "edev": "set ELECTRON_START_URL=https://localhost:3000 && electron .",
    "dev": "nf start",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron React App\""
  },
  "devDependencies": {
    "electron": "^2.0.9",
    "electron-packager": "^12.1.2",
    "foreman": "^3.0.1"
  },
  "homepage": "./"
}
